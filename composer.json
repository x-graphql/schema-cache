{
    "name": "x-graphql/schema-cache",
    "description": "Caching GraphQL schema with PSR-16",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "symfony/cache": "^6.4 || ^7.0",
        "symplify/easy-coding-standard": "^12.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "XGraphQL\\SchemaCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XGraphQL\\SchemaCache\\Test\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.2",
        "webonyx/graphql-php": "^15.9",
        "psr/simple-cache": "^2.0 || ^3.0"
    },
    "config": {
        "sort-packages": true
    }
}
